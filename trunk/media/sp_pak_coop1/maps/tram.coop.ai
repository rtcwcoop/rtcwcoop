player
{
	spawn
	{
		accum 0 bitset 0
		accum 0 bitset 1
		accum 0 bitset 2
		accum 0 bitset 3
		accum 0 bitset 4
		accum 1 set 0
		objectivesneeded 3

		abort_if_loadgame
		mu_start sound/music/l_briefing_1

	}

	playerstart
	{

		mu_fade 0 1500
		mu_queue sound/music/m_drums

	}

	trigger switch1on
	{
		objectivemet 1
		alertentity light1on
		trigger gear1 on1
		trigger gear2 on2		
		trigger gear3 on3
	}

	trigger starttram
	{
		trigger tramcar tramup
		trigger trammount mountup

		accum 0 abort_if_not_bitset 0
		accum 0 bitreset 0
		alertentity nazi6
		alertentity nazi7
	}

	trigger tramgo
	{
		trigger tramcar tramdown
		trigger trammount mountdown

		accum 0 abort_if_not_bitset 1
		accum 0 bitreset 1
		objectivemet 2
		alertentity nazi13
		alertentity nazi14
		alertentity nazi23
		accum 1 abort_if_equal 2
		accum 1 set 2
		mu_fade 0 500
		mu_queue sound/music/m_drums
	}

	trigger startlower
	{
		trigger tramcar tramcontinue
		trigger trammount mountcontinue
		accum 0 abort_if_not_bitset 2
		accum 0 bitreset 2
		alertentity nazi16
		alertentity nazi17
	}

	trigger powermid
	{
		accum 0 abort_if_not_bitset 3
		accum 0 bitreset 3
		alertentity light3on
		alertentity light4on
		trigger button2 resetbutton2
	}

	trigger returnmid
	{
		trigger tramcar tramreturn
		trigger trammount mountreturn
	}

	trigger openlocker1
	{
		trigger locker1 toggle
	}

	trigger upstairs
	{
		alertentity nazi1
	}

	trigger action1
	{
		alertentity nazi2
		alertentity nazi3
		alertentity nazi4
	}

	trigger lookout
	{
		trigger nazi2 shootwindows
	}

	trigger action2
	{
		trigger zone1 loweralarm
	}

	trigger action3
	{
		trigger nazi8 loweralarm8
		trigger nazi10 loweralarm10
	}

	trigger action4
	{
		alertentity nazi15
	}

	trigger action5
	{
		alertentity nazi18
		alertentity nazi19
		alertentity nazi24
		trigger nazi16 newspot16
	}

	trigger action6a // lower entrance
	{
		trigger nazi24 blowbarrel24
	}

	trigger action6b // upper entrance
	{
		trigger nazi24 clearorders24
	}

	trigger action7
	{
		alertentity nazi20
		alertentity nazi21
		alertentity nazi22
	}

	trigger action8
	{
		trigger nazi20 action20
		trigger nazi21 action21
		trigger nazi22 action22
	}

	trigger action9
	{
		trigger kesler dont_shoot
	}

	trigger explode_alarm
	{
		trigger zone1 explode_upper
	}

	trigger on_alarm
	{
		trigger zone1 upperalarm
		accum 1 abort_if_equal 1
		accum 1 set 1
		mu_fade 0 500
		mu_queue sound/music/m_alarm
	}

	trigger playalarm
	{
		accum 1 abort_if_equal 1
		accum 1 set 1
		mu_fade 0 500
		mu_queue sound/music/m_alarm
	}

	trigger playdrums
	{
		accum 1 abort_if_equal 2
		accum 1 set 2
		mu_fade 0 500
		mu_queue sound/music/m_drums
	}

	trigger button1used
	{
		trigger button1 pushbutton1
	}

	trigger button2used
	{
		trigger button2 pushbutton2
	}

	trigger endmap
	{
		trigger kesler follow_me
		changelevel village1 5000 persistent silent
	}

	trigger got_objective3
	{
		objectivemet 3
	}

	trigger cam1
	{
		startcam cs02_02
	}

	trigger clear_cam
	{
		stopcam
	}

	trigger cam2
	{
		startcam cs02_03
	}

	trigger cam3
	{
		startcam cs02_04
	}

	trigger cam4
	{
		startcam cs02_05
	}

	death
	{
		trigger locker1 midreset
	}
}

nazi1
{
	attributes
	{
		aim_accuracy 0.1
		starting_health 30
		attack_crouch 0.8
		fov 120
		inner_detection_radius 384
	}

	spawn
	{
		accum 0 set 0 // 1 = alerted
		accum 1 set 0 // 1 = others sighted player first
		setammo ammo_grenades 0
		takeweapon weapon_mp40

		wait 100
		gotomarker spotn1a
		facetargetangles spotn1a
		movetype crouch
		trigger nazi1 working1
	}

	trigger working1
	{
		accum 0 abort_if_not_equal 0
		playanim working_with_tools both
		trigger nazi1 working1
	}

	statechange relaxed alert
	{
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	statechange query alert
	{
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	statechange relaxed combat
	{
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	statechange query combat
	{
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	bulletimpact
	{
		statetype alert
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	inspectsoundstart
	{
		statetype alert
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	inspectbodystart
	{
		deny
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi1 alert1
	}

	inspectfriendlycombatstart
	{
		deny
	}

	trigger alert1
	{
		clearanim
		giveweapon weapon_luger
		setammo ammo_9mm 999
		selectweapon weapon_luger

		wait 50 player
		gotomarker spotn1b
		facetargetangles spotn1b
		wait 1000
	}

	trigger sighted1 // called by other ai's
	{
		accum 1 set 1
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		clearanim
		statetype alert
		giveweapon weapon_luger
		setammo ammo_9mm 999
		selectweapon weapon_luger

		wait 50 player
		gotomarker spotn1b
		facetargetangles spotn1b
		wait 1000
	}

	enemysight
	{
		accum 1 abort_if_not_equal 0
		accum 1 set 1
		playsound male_ze_american
	}

	death
	{
		accum 1 set 1
		trigger zone1 firstgroupdead
		trigger nazi3 nazi1dead

	}
}

