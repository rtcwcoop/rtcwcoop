	trigger map_start
	{
		//If something breaks at late player joining, move it here
		accum 0 bitset 2
		accum 0 bitreset 3
		
//I rewrote super1 accums due to a resetscript bug - Sage
		accum 1 bitreset 0	//COOP, Is super in place or low on health (rocket gun destroyed)
		accum 1 bitreset 1	//COOP, Is tank1 blown?
		accum 1 bitreset 2	//COOP, Is tank2 blown?
		accum 1 bitreset 3	//COOP, Is tank3 blown?
		accum 1 bitreset 4	//COOP, Is tank4 blown?
		accum 1 bitreset 5	//COOP, Is tank5 blown?
		accum 1 bitreset 6	//COOP, Are rockets working?
		accum 1 bitreset 7	//COOP, XTRA, Lockplayer?
		accum 1 bitreset 8	//COOP, XTRA, Player locked

		accum 2 set 0 // don't remove exit trigger

		objectivesneeded 3
		numsecrets 0

		mu_fade 0 1000
		mu_queue sound/music/x_atmos
	}

